{% for field in scope %}
  {% if field.first %}
    { {{ field.key | lowerFirst }} :: {% include "type-strict.ede" with field.value %}
  {% else %}
    , {{ field.key | lowerFirst }} :: {% include "type-strict.ede" with field.value %}
  {% endif %}
  {% for line in field.value.documentation %}
    {% if line.value /= "" %}
      {% if line.first %}
      -- ^ {{ line.value }}
      {% else %}
      -- {{ line.value }}
      {% endif %}
    {% endif %}
  {% endfor %}
{% endfor %}
