{% case scope.value.shape_name %}
{% when "Metadata" %}
 hdrs "x-amz-meta-" hs
{% else %}
  {% if scope.value.streaming %}
 pure bdy
  {% else %}
    {% if scope.value.payload %}
 xml "{{ scope.key | dropLower }}" doc
    {% else %}
      {% if scope.value.location == "header" %}
 hdr "{{ scope.value.location_name }}" hs
      {% else %}
 xml "{{ scope.key | dropLower }}" doc
      {% endif %}
    {% endif %}
  {% endif %}
{% endcase %}
