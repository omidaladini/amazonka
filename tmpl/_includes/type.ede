{% case scope.type %}
{% when "map" %}
 HashMap {{ scope.key.shape_name }} {{ scope.value.shape_name }}
{% when "list" %}
  {% if scope.length > 0 %}
 NonEmpty {{ scope.item.shape_name }}
  {% else %}
 [{{ scope.item.shape_name }}]
  {% endif %}
{% else %}
  {% if scope.required %}
 !{{ scope.shape_name }}
  {% else %}
 Maybe {{ scope.shape_name }}
  {% endif %}
{% endcase %}
